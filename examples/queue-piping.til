print "START"

set q [queue]

print "Test 1"
range 1 | send $q
print [pop $q] " should be 0"
print [pop $q] " should be 1"

print "Test 2"
range 2 | send $q
receive $q | foreach a {
    print "Received $a"
    if ($a == 2) { break }
}

